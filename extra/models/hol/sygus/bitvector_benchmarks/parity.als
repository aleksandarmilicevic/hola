module parity<spec>

open ../synth2[spec]

--------------------------------------------------------------------------------
-- Variables
--------------------------------------------------------------------------------
one sig A, B, C, D extends BoolVar {}

--------------------------------------------------------------------------------
-- Helpers
--------------------------------------------------------------------------------
fun parity[a, b, c, d: Bit]: Bit {
  Xor[Not[Xor[a, b]], 
      Not[Xor[c, d]]]
}

fact {
  IntVarVal = Int
}
